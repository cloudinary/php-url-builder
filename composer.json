{
  "name": "cloudinary/url-builder",
  "version": "0.1.0-beta",
  "description": "Cloudinary PHP URL Builder SDK",
  "keywords": [
    "cloudinary",
    "sdk",
    "cloud",
    "image management",
    "cdn"
  ],
  "type": "library",
  "homepage": "https://github.com/cloudinary/php-url-builder",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Cloudinary",
      "homepage": "https://github.com/cloudinary/php-url-builder/graphs/contributors"
    }
  ],
  "support": {
    "email": "info@cloudinary.com",
    "issues": "https://github.com/cloudinary/php-url-builder/issues"
  },
  "require": {
    "php": ">=8.0",
    "cloudinary/transformation-builder-sdk":  "^1",
    "guzzlehttp/psr7": "^1|^2",
    "ext-json": "*",
    "monolog/monolog": "^1|^2",
    "psr/log" : "^1|^2|^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpmd/phpmd": "*",
    "squizlabs/php_codesniffer": "3.*",
    "friendsofphp/php-cs-fixer": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "consolidation/robo": "~1"
  },
  "autoload": {
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ],
    "psr-4": {
      "Cloudinary\\Test\\": "tests/"
    }
  }
}
